{
  "extends": [
	"@abhijithvijayan/eslint-config/typescript"
  ],
  "parserOptions": {
	"project": "./tsconfig.eslint.json",
	"sourceType": "module"
  },
  "env": {
	"jest": true
  },
  "rules": {
	"no-console": "off",
	"no-param-reassign": "warn",
	"@typescript-eslint/no-use-before-define": "warn",
	"@typescript-eslint/no-explicit-any": "warn"
  }
}
